<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.yddmall.mapper.ItemSkuMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.yddmall.entity.ItemSku">
        <id column="sku_id" property="skuId" />
        <result column="item_id" property="itemId" />
        <result column="sku_name" property="skuName" />
        <result column="properties_name" property="propertiesName" />
        <result column="price" property="price" />
        <result column="quantity" property="quantity" />
        <result column="barcode" property="barcode" />
        <result column="sku_pic" property="skuPic" />
        <result column="status" property="status" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        sku_id, item_id, sku_name, properties_name, price, quantity, barcode, sku_pic, status
    </sql>

</mapper>
