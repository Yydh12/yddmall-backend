<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.yddmall.mapper.ItemPropMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.yddmall.entity.ItemProp">
        <id column="pid" property="pid" />
        <result column="name" property="name" />
        <result column="is_key_prop" property="isKeyProp" />
        <result column="is_sale_prop" property="isSaleProp" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        pid, name, is_key_prop, is_sale_prop
    </sql>

</mapper>
